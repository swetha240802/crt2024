import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        float a;
        a=s.nextFloat();
        int r=(int)a;
        System.out.println((int)a);
        System.out.printf("%.1f\n",Math.ceil(a));
        System.out.printf("%.1f\n",Math.floor(a));
    }
}

    
