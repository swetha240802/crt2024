import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int x,a,b;
        Scanner s=new Scanner(System.in);
        x=s.nextInt();
        a=s.nextInt();
        b=s.nextInt();
        int t=(x*a)-(x*b);
        int profit=t-100;
        System.out.println(profit);
        
    }
}
