#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int age,wt;
    float h;
    scanf("%d%d",&age,&wt);
    scanf("%f",&h);
    if(age>=18 && wt>=45 && h>=5.5)
    {
        printf("He or She can donate blood\n");
    }
    else
    {
        printf("He or She unable to donate blood\n");
    }
return 0;
}
