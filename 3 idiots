import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
                  int x1,y1,x2,y2;
        Scanner s=new Scanner(System.in);
        x1=s.nextInt();
        y1=s.nextInt();
        x2=s.nextInt();
        y2=s.nextInt();
        float m1=(float)(x1+x2)/2;
        float m2=(float)(y1+y2)/2;
        System.out.println("Binoy's house is located at ("+m1+","+m2+")");
    }
}
