#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
int n;
    scanf("%d",&n);
    if(n<=200)
    {
        printf("Rs.%d",(int) (n*0.5) );
    }
    else if(n<=400)
    {
        printf("Rs.%d",(int) (n*0.65)+100);
    }
    else if(n<=600)
    {
        printf("Rs.%d",(int) (n*0.80)+200);
    }
    else if(n>600)
    {
        printf("Rs.%d",(int)(n*1.25)+425);
    }

  return 0;
}
